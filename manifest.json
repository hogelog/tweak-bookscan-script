{
  "name": "Tweak bookscan",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Tweak bookscan web site",
  "content_scripts": [
    {
      "matches": [
        "https://system.bookscan.co.jp/bookshelf_all_list.php*"
      ],
      "js": ["jquery-2.1.0.min.js", "all_list.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://system.bookscan.co.jp/bookoptimize.php*"
      ],
      "js": ["jquery-2.1.0.min.js", "bookoptimize.js"],
      "run_at": "document_end"
    }
  ]
}
